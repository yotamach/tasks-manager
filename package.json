  "dependencies": {
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.3.10",
    "babel-plugin-root-import": "^6.6.0",
    "cypress": "^7.5.0",
    "react": "^17.0.1"
  }
