{
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "es6": true
    },
    "plugins": ["import","file-progress"],
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "globals": {
      "exampleGlobalVariable": true
    },
    "rules": {
        "file-progress/activate": 1,
        "indent": [
            "error",
            "tab"
        ],
        "no-eval": "error",
        "import/first": "error"
    }
}